            <div class="row">
              <div class="col-5">
                <img class="img-fluid d-block" src=<%=jsonProductPrices["prod_NEGSEJF0dgdFGv"].img%>>
              </div>
              <div class="col-7">
                <div class="row row-cols-1">
                  <div class="col">
                    <h5 class="subtituloPlan"><%=price.name%></h4>
