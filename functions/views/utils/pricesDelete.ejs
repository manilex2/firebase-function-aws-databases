              <h2 class="valuePrecio">$<%=price.value %></h2>
              <p>/<%=price.type %></p>
            </div>
          </div>
          <div class="col">
            <input type="hidden" name="idPrice" value=<%=price.id %>>
            <input class="btn btn-primary w-100" type="submit" value="Suscribirse">
          </div>
        </div>
      </form>
    </div>
